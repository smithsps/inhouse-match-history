{
	"name": "inhouse-match-history",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"schema": "wrangler d1 execute inhouse-matches --file ./db_schema/schema.sql --local",
		"schema:prod": "wrangler d1 execute inhouse-matches --file ./db_schema/schema.sql --remote"
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "^3.787.0",
		"@aws-sdk/s3-request-presigner": "^3.797.0",
		"@iconify/json": "^2.2.331",
		"@iconify/tailwind4": "^1.0.6",
		"@sveltejs/adapter-cloudflare": "^7.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22.14.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.5",
		"wrangler": "^4.12.0"
	},
	"dependencies": {
		"@lucia-auth/adapter-sqlite": "^3.0.2",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"arctic": "^3.6.0",
		"lucia": "^3.2.2"
	}
}
